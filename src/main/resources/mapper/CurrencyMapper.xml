<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.felix.springboottour.mapper.CurrencyMapper">

    <insert id="addCurrency">
        INSERT INTO currency(currency_id, currency_name) VALUES (#{currencyId}, #{currencyName})
    </insert>

</mapper>
